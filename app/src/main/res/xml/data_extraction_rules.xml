<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>

    <cloud-backup disableIfNoEncryptionCapabilities="false">
        <include
            domain="file"
            path="datastore/user_prefs.pb" />
    </cloud-backup>

    <device-transfer>
        <include
            domain="file"
            path="datastore/user_prefs.pb" />
    </device-transfer>

    <cross-platform-transfer platform="ios">
        <include
            domain="file"
            path="datastore/user_prefs.pb" />
    </cross-platform-transfer>

</data-extraction-rules>